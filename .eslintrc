{
  "extends": "react-app",
  "rules": {
    "react/react-in-jsx-scope": "off",
    "jsx-a11y/href-no-hash": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "jsx-a11y/alt-text": "off",
    "jsx-a11y/accessible-emoji": "off",
    "no-console": "warn",
    "default-case":"off",
    "valid-jsdoc": "off"
  },
  "globals":{
      "$api":true
  }
}
